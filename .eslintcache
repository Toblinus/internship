ocuments/trello/src/components/SortingElement.tsx",["218"],"import React, { RefObject, useRef } from 'react';\nimport { useDrag, useDrop, XYCoord } from 'react-dnd';\n\nexport type Props = {\n    children: JSX.Element;\n    id: number;\n    move: (a: number, b: number) => void;\n    type: string;\n    allowDropRefs: any[]\n};\n\nconst SortingElement: React.FC<Props> = ({ children, id, move, type, allowDropRefs }) => {\n    const ref = useRef<HTMLDivElement>(null);\n    \n    const [{ opacity }, drag] = useDrag({\n        item: { type, id },\n        canDrag: true,\n        collect: (monitor) => ({\n            opacity: monitor.isDragging() ? 0 : 1\n        })\n    });\n\n    \n    const [, drop] = useDrop({\n        accept: type, \n        hover: (item: {id: number, type: string}, monitor) => {\n            if (!ref.current) {\n                return;\n            }\n            \n            const dragIndex = item.id;\n            const hoverIndex = id;\n            \n            if(dragIndex === hoverIndex) {\n                return;\n            }\n            \n            const hoverBoundingRect = ref.current?.getBoundingClientRect()\n            const hoverMiddleY = (hoverBoundingRect.bottom - hoverBoundingRect.top) / 2;\n            const hoverMiddleX = (hoverBoundingRect.right - hoverBoundingRect.left) / 2;\n            \n            const clientOffset = monitor.getClientOffset();\n            \n            const hoverClientY = (clientOffset as XYCoord).x - hoverBoundingRect.top;\n            const hoverClientX = (clientOffset as XYCoord).y - hoverBoundingRect.right;\n            \n            \n            if (dragIndex < hoverIndex && (hoverClientY < hoverMiddleY || hoverClientX > hoverMiddleX)) {\n                return;\n            }\n            \n            if (dragIndex > hoverIndex && (hoverClientY > hoverMiddleY || hoverClientX < hoverMiddleX)) {\n                return;\n            }\n            \n            move?.(hoverIndex, dragIndex);\n            item.id = hoverIndex;\n        }\n    })\n    \n    drag(drop(ref));\n    React.useEffect(() => {\n        if(!Array.isArray(allowDropRefs)){\n            return;\n        }\n        for(let i = 0; i < (allowDropRefs.length || 0); i++) {\n            allowDropRefs[i] = drop(allowDropRefs[i]);\n        }\n    }, [allowDropRefs, drop]);\n    \n    return (<div \n        style={{ opacity }}\n        ref={ref}\n        >\n            { children }\n    </div>);\n}\n\nexport default SortingElement;","/home/user/Documents/trello/src/components/Modal/ModalBase/index.tsx",[],"/home/user/Documents/trello/src/components/PopoutWrapper/index.tsx",["219"],"import React from \"react\";\nimport PropTypes from \"prop-types\"\n\nimport joinClasses from '../../helpers/joinClasses';\n\nimport './style.css';\n\nexport type Props = {\n    className?: string;\n    children: JSX.Element;\n    onClick?: () => void;\n}\n\nconst PopoutWrapper: React.FC<Props> = ({ children, onClick = () => {}, className = '' }) => {\n    return (<div className={joinClasses('popout-wrapper', className)} onClick={onClick} >\n        { children }\n    </div>);\n}\nexport default PopoutWrapper;",{"ruleId":"220","replacedBy":"221"},{"ruleId":"222","replacedBy":"223"},{"ruleId":"220","replacedBy":"224"},{"ruleId":"222","replacedBy":"225"},{"ruleId":"226","severity":1,"message":"227","line":13,"column":1,"nodeType":"228","endLine":17,"endColumn":2},{"ruleId":"226","severity":1,"message":"229","line":5,"column":1,"nodeType":"228","endLine":39,"endColumn":3},{"ruleId":"226","severity":1,"message":"229","line":6,"column":1,"nodeType":"228","endLine":9,"endColumn":2},{"ruleId":"226","severity":1,"message":"229","line":4,"column":1,"nodeType":"228","endLine":7,"endColumn":2},{"ruleId":"226","severity":1,"message":"229","line":4,"column":1,"nodeType":"228","endLine":7,"endColumn":2},{"ruleId":"220","replacedBy":"230"},{"ruleId":"222","replacedBy":"231"},{"ruleId":"226","severity":1,"message":"229","line":5,"colum